{
    "properties": {
      "displayName": "Custom - Deny HTTP on storage accounts",
      "policyType": "Custom",
      "mode": "All",
      "parameters": {},
      "policyRule": {
        "if": {
          "field": "Microsoft.Storage/storageAccounts/supportsHttpsTrafficOnly",
          "equals": "False"
        },
        "then": {
          "effect": "deny"
        }
      }
    }
  }
